<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FinMinds"
        tools:targetApi="31">
        <activity
            android:name=".InvRetPlanActivity"
            android:exported="false"
            android:label="@string/title_activity_inv_ret_plan"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".ManageDebtActivity"
            android:exported="false"
            android:label="@string/title_activity_manage_debt"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".BuildingcsActivity"
            android:exported="false"
            android:label="@string/title_activity_buildingcs"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".BudgetingActivity"
            android:exported="false"
            android:label="@string/title_activity_budgeting"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".TaxActivity"
            android:exported="false"
            android:label="@string/title_activity_tax"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".TopicActivity"
            android:exported="true"
            android:label="@string/title_activity_topic"
            android:theme="@style/Theme.FinMinds" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.FinMinds">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>